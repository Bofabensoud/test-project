    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(-10,10,400)\n",
    "psi = np.exp(-x**2/2.0)\n",
    "plt.plot(x, np.abs(psi)**2)\n",
    "plt.title('Initial Gaussian wavepacket')\n",
    "plt.xlabel('x')\n",
    "plt.show()"
   ]
  }
